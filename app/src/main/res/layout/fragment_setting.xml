<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/displaybg">
    <TextView
        android:id="@+id/textView"
        android:layout_width="208dp"
        android:layout_height="57dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="64dp"
        android:adjustViewBounds="true"
        android:fontFamily="@font/monsemibold"
        android:text="@string/Settings"
        android:textColor="#000"
        android:textSize="45sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.287"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/notifications"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="350dp"
        android:layout_height="89dp"
        android:adjustViewBounds="true"
        android:background="@drawable/setbox"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.722" />

    <Button
        android:id="@+id/display"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="350dp"
        android:layout_height="89dp"
        android:adjustViewBounds="true"
        android:background="@drawable/setbox"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.565" />

    <Button
        android:id="@+id/about"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="350dp"
        android:layout_height="89dp"
        android:adjustViewBounds="true"
        android:background="@drawable/setbox"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.408" />

    <Button
        android:id="@+id/account"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="350dp"
        android:layout_height="89dp"
        android:adjustViewBounds="true"
        android:background="@drawable/setbox"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.252" />


    <ImageView
        android:id="@+id/accounticon"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="16dp"
        android:adjustViewBounds="true"
        android:background="@drawable/acicon"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.288"
        tools:targetApi="donut" />

    <ImageView
        android:id="@+id/seticon"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="16dp"
        android:adjustViewBounds="true"
        android:background="@drawable/seticon"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.437"
        tools:targetApi="donut" />

    <ImageView
        android:id="@+id/displayimage"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginStart="44dp"
        android:adjustViewBounds="true"
        android:background="@drawable/displayicon"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.56"
        tools:targetApi="donut" />

    <ImageView
        android:id="@+id/notification"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginStart="44dp"
        android:adjustViewBounds="true"
        android:background="@drawable/notiicon"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.711"
        tools:targetApi="donut" />

    <TextView
        android:id="@+id/accounttext"
        android:layout_width="167dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:adjustViewBounds="true"
        android:fontFamily="@font/alithin"
        android:text="Account"
        android:textColor="#5E5C5C"
        android:textSize="27sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.346"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.286" />

    <TextView
        android:id="@+id/abouttext"
        android:layout_width="158dp"
        android:layout_height="39dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:adjustViewBounds="true"
        android:fontFamily="@font/alithin"
        android:text="About"
        android:textColor="#5E5C5C"
        android:textSize="27sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.442" />

    <TextView
        android:id="@+id/displaytext"
        android:layout_width="158dp"
        android:layout_height="39dp"
        android:layout_marginStart="16dp"
        android:adjustViewBounds="true"
        android:fontFamily="@font/alithin"
        android:text="Display"
        android:textColor="#5E5C5C"
        android:textSize="27sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.566" />

    <TextView
        android:id="@+id/notitext"
        android:layout_width="158dp"
        android:layout_height="39dp"
        android:layout_marginStart="16dp"
        android:adjustViewBounds="true"
        android:fontFamily="@font/alithin"
        android:text="Notifications"
        android:textColor="#5E5C5C"
        android:textSize="27sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.718" />


    <Button
        android:id="@+id/logout"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="165dp"
        android:layout_height="44dp"
        android:adjustViewBounds="true"
        android:background="@drawable/logout"
        android:fontFamily="@font/alireg"
        android:text="LOG OUT"
        android:textColor="#ffff"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.842" />


</androidx.constraintlayout.widget.ConstraintLayout>
